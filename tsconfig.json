{
    "include": ["src/**/*.ts"],
    "compilerOptions": {
		"lib": ["DOM", "ES5", "ES6", "ES7"],
		"module": "ESNext",
		"noImplicitAny": true,
		"moduleResolution": "node",
		"isolatedModules": true,
		"strictNullChecks": true,
		"strict": true,
		"sourceMap": true,
		"target": "ES6",
        "outDir": "dist/esm",
		"noEmit": false,
    }
}
